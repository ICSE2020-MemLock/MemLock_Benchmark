msgstr "  --section-alignment <tamaño>       Establece la alineación de la sección\n"

#: emultempl/pe.em:442
#, c-format
msgid "  --stack <size>                     Set size of the initial stack\n"
msgstr "  --stack <size>                     Establece el tamaño de la pila inicial\n"

